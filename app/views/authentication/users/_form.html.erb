<%= form_with model:@user do |form|%>
<div>
<%= form.label :nmae%>
<%= form.text_field :name%>
</div>
<div>
<%= form.label :username%>
<%= form.text_field :username%>
</div>

<div>
<%= form.label :password%>
<%= form.password_field :password%>
</div>

<div>
<%= form.label :Admin?%>
<%= form.select :admin,options_for_select([["No",false],["Si",true]])%>
</div>


<%= form.submit%>

<% end %>